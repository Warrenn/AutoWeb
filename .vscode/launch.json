{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Launch Program",
            "localRoot": "${workspaceRoot}/auto-web",
            "port": 9229,
            "remoteRoot": "/var/task",
            "protocol": "inspector",
            "stopOnEntry": false,
            "address": "localhost"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha All Files",
            "program": "${workspaceRoot}/auto-web/node_modules/mocha/bin/_mocha",
            "args": [
                "--timeout",
                "999999",
                "--colors",
                "${workspaceRoot}/auto-web/tests/unit"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}